# Backend selection
# BROWSER_BACKEND=playwright          # "playwright", "patchright", or "remote"
# BROWSER_WS_URL=ws://localhost:3000  # Required when BROWSER_BACKEND=remote

# Browser settings
# BROWSER_HEADLESS=true          # Set to "false" for debugging
# PAGE_TIMEOUT=30000             # Page load timeout in ms
# INTERCEPT_TIMEOUT=15000        # Max wait for fetchReviews response in ms
# REQUEST_DELAY=2.0              # Delay between paginated requests (seconds)
# MAX_PAGES=20                   # Max review pages to fetch
# SCRAPER_RETRIES=3              # Retry attempts for page loads
# SCRAPER_RETRY_DELAY=2.0        # Base retry delay (seconds)
# BROWSER_LOCALE=ru-RU           # Browser locale

# Tool filtering
# ENABLED_TOOLS=get_reviews,get_company_info,get_company_summary
